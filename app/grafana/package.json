{
    "name": "grafana-honeycomb",
    "version": "1.2.1",
    "scripts": {
        "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
        "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
        "test": "jest --watch --onlyChanged",
        "test:ci": "jest --passWithNoTests --maxWorkers 4",
        "typecheck": "tsc --noEmit",
        "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
        "lint:fix": "yarn run lint --fix",
        "e2e": "playwright test",
        "server": "docker compose up --build",
        "package": "rm -rf jpl-honeycomb-panel && rm -rf jpl-honeycomb-panel.zip && cp -R dist jpl-honeycomb-panel && zip -r jpl-honeycomb-panel.zip jpl-honeycomb-panel",
        "sign": "npx --yes @grafana/sign-plugin@latest"
    },
    "author": "JPL",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@emotion/styled": "^11.13.0",
        "@gov.nasa.jpl.honeycomb/common": "^1.1.15",
        "@gov.nasa.jpl.honeycomb/core": "^1.1.15",
        "@gov.nasa.jpl.honeycomb/extensions": "^1.1.15",
        "@gov.nasa.jpl.honeycomb/framerate-optimizer": "^1.1.15",
        "@gov.nasa.jpl.honeycomb/scene-viewers": "^1.1.15",
        "@gov.nasa.jpl.honeycomb/scheduling-utilities": "^1.1.15",
        "@gov.nasa.jpl.honeycomb/telemetry-animator": "^1.1.15",
        "@grafana/eslint-config": "^7.0.0",
        "@grafana/tsconfig": "^2.0.0",
        "@minoru/react-dnd-treeview": "^3.4.4",
        "@playwright/test": "^1.41.2",
        "@rjsf/core": "^5.18.6",
        "@rjsf/utils": "^5.18.6",
        "@rjsf/validator-ajv8": "^5.18.1",
        "@testing-library/jest-dom": "6.1.4",
        "@testing-library/react": "14.0.0",
        "@types/jest": "^29.5.0",
        "@types/json-schema": "^7.0.15",
        "@types/lodash": "^4.14.194",
        "@types/node": "^20.8.7",
        "@types/react-router-dom": "^5.2.0",
        "@types/testing-library__jest-dom": "5.14.8",
        "@types/three": "^0.169.0",
        "ajv": "7.1.0",
        "glob": "^10.2.7",
        "identity-obj-proxy": "3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "path-browserify": "^1.0.1",
        "prettier": "^2.8.7",
        "process": "^0.11.10",
        "raw-loader": "^4.0.2",
        "react-dnd": "^16.0.1",
        "sass": "1.63.2",
        "three": "^0.169.0",
        "three-mesh-bvh": "^0.9.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.5.4",
        "uid": "^2.0.2",
        "urdf-loader": "^0.12.3"
    },
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@emotion/css": "11.10.6",
        "@grafana/data": "^11.2.0",
        "@grafana/runtime": "^11.2.0",
        "@grafana/schema": "^10.4.0",
        "@grafana/ui": "^11.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tslib": "2.5.3"
    },
    "packageManager": "yarn@1.22.19"
}
